.toolbar{
	margin-bottom: 10px;
}
.route{
	padding-left: 16px;
	height: 40px;
	line-height: 40px;
	border-bottom: 1px solid #eee;
	color: #666;
}
.page-container {
	float:right;
	margin-bottom: 20px;
}
.folder-container{
	margin: 20px 0;
}
.folder,
.widget{
	display: inline-block;
	margin-right: 10px;
	padding-top: 64px;
	width: 100px;
	height: 88px;
	overflow:hidden;
	line-height: 24px;
	text-align:center;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.folder{
	background:url(../img/folder.png) center 0 no-repeat;
}
.widget{
	background:url(../img/zip.png) center 15px no-repeat;
}
.folder:link,
.folder:visited,
.widget:link,
.widget:visited{
	color: #333;
	text-decoration : none;
	background-color: transparent;
}
.folder:hover,
.folder:active,
.widget:hover,
.widget:active{
	color: #333;
	text-decoration : none;
	background-color: #badeff;
}
.seled:link,
.seled:visited,
.seled:hover,
.seled:active{
	background-color: #badeff !important;
}